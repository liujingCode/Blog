{
    "title": "爱生活爱Coding",
    "description": "不重复造轮子",
    "author": "liujing",
    "language": "zh-hans",
    "extension": null,
    "generator": "site",
    "links": {
        "sidebar": {
            "有问题找百度": "https://www.baidu.com"
        }
    },
    "plugins": [
        "-highlight",
        "-sharing",
        "-search",
        "search-pro",
        "pageview-count",
        "copy-code-button",
        "github",
        "sharing-plus",
        "splitter",
        "toggle-chapters",
        "tbfed-pagefooter",
        "prism",
        "insert-logo",
        "styles-less"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "github": {
            "url": "https://github.com/liujingCode"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © liujing 2019",
            "modify_label": "创建时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "sharing": {
            "facebook": true,
            "twitter": true,
            "weibo": true,
            "qq": true,
            "all": [
                "douban",
                "google",
                "qzone",
                "linkedin"
            ]
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-solarizedlight.css"
            ],
            "lang": {
                "flow": "typescript"
            }
        },
        
        "insert-logo": {
        "url": "http://www.fuming.site/dist/avator.jpg",
        "style": "background: none; max-height: 30px; min-height: 30px"
        }
    }
}
